{
    "restApiRoot": "/api",
    "host": "0.0.0.0",
    "domain": "http://localhost:3000",
    "siteDomain": "http://localhost:3000",
    "thumbUrl": "C:/FFmpeg/bin/",
    "programFFmpegName": [
        "ffmpeg.exe",
        "ffprobe.exe"
    ],
    "port": 3000,
    "remoting": {
        "context": false,
        "rest": {
            "handleErrors": false,
            "normalizeHttpPath": false,
            "xml": false
        },
        "json": {
            "strict": false,
            "limit": "100kb"
        },
        "urlencoded": {
            "extended": true,
            "limit": "100kb"
        },
        "cors": false
    }
}