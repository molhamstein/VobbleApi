{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "user",
        "foreignKey": "userId"
      }
    }
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "user": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": true
        }
      }
    }
  },
  "Email": {
    "dataSource": "email"
  },
  "uploadFile": {
    "dataSource": "files",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "upload": true,
          "download": true
        }
      }
    }
  },
  "shore": {
    "dataSource": "db",
    "public": true
  },
  "bottle": {
    "dataSource": "db",
    "public": true
  },
  "bottleUserseen": {
    "dataSource": "db",
    "public": false
  },
  "country": {
    "dataSource": "db",
    "public": true
  },
  "typeGoods": {
    "dataSource": "db",
    "public": true
  },
  "product": {
    "dataSource": "db",
    "public": true
  },
  "item": {
    "dataSource": "db",
    "public": true
  },
  "report-type": {
    "dataSource": "db",
    "public": true
  },
  "report": {
    "dataSource": "db",
    "public": true
  }
}
