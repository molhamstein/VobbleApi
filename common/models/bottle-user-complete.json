{
  "name": "bottleUserComplete",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [{
      "relation": "user"
    }]
  },
  "properties": {
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "bottle": {
      "type": "belongsTo",
      "model": "bottle",
      "foreignKey": "bottleId",
      "options": {
        "nestRemoting": true
      }
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {
    "seenAndComplete": {
      "accepts": [{
          "arg": "seen",
          "type": "array",
          "required": true,
          "description": "Seen Bottle Array"
        },
        {
          "arg": "complete",
          "type": "array",
          "required": true,
          "description": "Complete Bottle Array"
        },
        {
          "arg": "req",
          "type": "object",
          "http": {
            "source": "req"
          }
        }
      ],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "get one mthode with some logic",
      "http": [{
        "path": "/seenAndComplete",
        "verb": "post"
      }]
    }
  }
}
