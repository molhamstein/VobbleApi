{
  "name": "notificationCenter",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [{
      "relation": "owner"
    }]
  },
  "properties": {
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    },
    "isSeen": {
      "type": "boolean",
      "required": true,
      "defaultFn": false
    },
    "title": {
      "type": "string",
      "required": true
    },
    "text": {
      "type": "string",
      "required": true
    },
    "img": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    }
  },
  "acls": [],
  "methods": {
    "getMyCenterNotification": {
      "accepts": [{
        "arg": "context",
        "type": "object",
        "http": {
          "source": "context"
        }
      }],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "get my info",
      "http": [{
        "path": "/getMyCenterNotification",
        "verb": "get"
      }]
    },
    "makeNotificationSeen": {
      "accepts": [{
        "arg": "notificationIds",
        "type": "array",
        "required": true
      }, {
        "arg": "context",
        "type": "object",
        "http": {
          "source": "context"
        }
      }],
      "returns": [{
        "arg": "result",
        "type": "number",
        "root": true,
        "description": ""
      }],
      "description": "get my info",
      "http": [{
        "path": "/makeNotificationSeen",
        "verb": "put"
      }]
    }
  }
}
