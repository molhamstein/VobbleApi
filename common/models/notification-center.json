{
  "name": "notificationCenter",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [{
      "relation": "owner"
    }]
  },
  "properties": {
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    },
    "isSeen": {
      "type": "boolean",
      "defaultFn": false
    },
    "title": {
      "type": "string",
      "required": true
    },
    "text": {
      "type": "string",
      "required": true
    },
    "img": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    }
  },
  "acls": [],
  "methods": {}
}
