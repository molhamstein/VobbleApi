{
  "name": "item",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "storeType": {
      "type": "string",
      "enum": ["red", "green", "blue"],
      "required": true
    },
    "storeToken": {
      "type": "string",
      "required": true
    },
    "isConsumed": {
      "type": "boolean",
      "required": true
    },
    "valid": {
      "type": "boolean",
      "required": true
    },
    "startAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "endAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "product": {
      "type": "belongsTo",
      "model": "product",
      "foreignKey": "productId"
    },
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    }
  },
  "acls": [],
  "methods": {}
}