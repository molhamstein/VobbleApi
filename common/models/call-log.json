{
  "name": "callLog",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [{
        "relation": "owner"
      },
      {
        "relation": "relatedUser"
      }
    ]
  },
  "replaceOnPUT": false,
  "properties": {
    "conversationId": {
      "type": "string",
      "required": true
    },
    "startAt": {
      "type": "date",
      "required": false
    },
    "endAt": {
      "type": "date",
      "required": false
    },
    "duration": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "ringing"
    },
    "cost": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "relatedUser": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "relatedUserId"
    },
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    }
  },
  "acls": [{
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY",
      "property": "getFilterCallLog"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "countFilterCallLog"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "exportFilterCallLog"
    }
  ],
  "methods": {
    "updateCallLog": {
      "accepts": [{
        "arg": "id",
        "type": "string",
        "required": true,
        "description": "call log id",
        "http": {
          "source": "path"
        }
      }, {
        "arg": "startAt",
        "type": "date",
        "required": false,
        "description": "startAt"
      }, {
        "arg": "endAt",
        "type": "date",
        "required": false,
        "description": "endAt"
      }, {
        "arg": "isFinish",
        "type": "boolean",
        "required": false,
        "description": "isFinish"
      }, {
        "arg": "status",
        "type": "string",
        "required": false,
        "description": "status"
      }, {
        "arg": "isReview",
        "type": "boolean",
        "required": true,
        "description": "isReview"
      }],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "",
      "http": [{
        "path": "/:id/updateCallLog",
        "verb": "put"
      }]
    },
    "countFilterCallLog": {
      "accepts": [{
        "arg": "where",
        "type": "object",
        "required": true,
        "description": "filter item"
      }],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "",
      "http": [{
        "path": "/countFilterCallLog",
        "verb": "get"
      }]
    },
    "getFilterCallLog": {
      "accepts": [{
        "arg": "filter",
        "type": "object",
        "required": false,
        "description": "filter Item"
      }],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "filter On the Call Log",
      "http": [{
        "path": "/getFilterCallLog",
        "verb": "get"
      }]
    },
    "exportFilterCallLog": {
      "accepts": [{
        "arg": "where",
        "type": "object",
        "required": false,
        "description": "filter Item"
      }],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "filter On the Call Log",
      "http": [{
        "path": "/exportFilterCallLog",
        "verb": "get"
      }]
    }
  }
}
