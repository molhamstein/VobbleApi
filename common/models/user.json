        "root": true,
        "description": ""
      }],
      "description": "",
      "http": [{
        "path": "/blockList",
        "verb": "get"
      }]
    },
    "me": {
      "accepts": [{
        "arg": "context",
        "type": "object",
        "http": {
          "source": "context"
        }
      }],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "get my info",
      "http": [{
        "path": "/me",
        "verb": "get"
      }]
    },
    "deactiveUser": {
      "accepts": [{
        "arg": "id",
        "type": "string",
        "required": true,
        "description": ""
      }],
      "returns": [],
      "description": "",
      "http": [{
        "path": "/deactiveUser",
        "verb": "delete"
      }]
