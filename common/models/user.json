    "block": {
      "accepts": [{
          "arg": "userId",
          "type": "string",
          "required": true,
          "description": ""
        },
        {
          "arg": "context",
          "type": "object",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "block user",
      "http": [{
        "path": "/block",
        "verb": "post"
      }]
    },
    "unBlock": {
      "accepts": [{
          "arg": "userId",
          "type": "string",
          "required": true,
          "description": ""
        },
        {
          "arg": "context",
          "type": "object",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": [{
        "arg": "result",
        "type": "boolean",
        "root": true,
        "description": ""
      }],
      "description": "unblock user",
      "http": [{
        "path": "/unblock",
        "verb": "delete"
      }]
    },
    "myListBlock": {
      "accepts": [{
        "arg": "context",
