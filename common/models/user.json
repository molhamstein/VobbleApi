{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "ttl": 31536000000
  },
  "replaceOnPUT": false,
  "hidden": [
    "password",
    "verificationToken",
    "emailVerified",
    "realm"
  ],
  "scope": {
    "include": [{
      "relation": "country"
    }]
  },
  "properties": {
    "gender": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "string"
    },
    "totalBottlesThrown": {
      "type": "number",
      "default": 0
    },
    "repliesBottlesCount": {
      "type": "number",
      "default": 0
    },
    "repliesReceivedCount": {
      "type": "number",
      "default": 0
    },
    "foundBottlesCount": {
      "type": "number",
      "default": 0
    },
    "extraBottlesCount": {
      "type": "number",
      "default": 0
    },
    "bottlesCount": {
      "type": "number",
      "default": 3
    },
    "email": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active"
    },
    "registrationCompleted": {
      "type": "boolean",
      "required": true,
      "default": "false"
    },
    "homeTutShowed": {
      "type": "boolean",
      "required": true,
      "default": "false"
    },
    "ChatTutShowed": {
      "type": "boolean",
      "required": true,
      "default": "false"
    },
    "tut3Showed": {
      "type": "boolean",
      "required": true,
      "default": "false"
    },
    "typeLogIn": {
      "type": "string",
      "default": "registration"
    },
    "socialId": {
      "type": "string"
    },
    "nextRefill": {
      "type": "date"
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "lastLogin": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping",
      "options": {
        "disableInclude": true
      }
    },
    "notifications": {
      "type": "hasMany",
      "model": "notification",
      "foreignKey": "ownerId",
      "options": {
        "disableInclude": true
      }
    },
    "country": {
      "type": "belongsTo",
      "model": "country",
      "foreignKey": "ISOCode"
    },
    "items": {
      "type": "hasMany",
      "model": "item",
      "foreignKey": "ownerId"
    },
    "myBottles": {
      "type": "hasMany",
      "model": "bottle",
      "foreignKey": "ownerId"
    },
    "blocking": {
      "type": "hasMany",
      "model": "block",
      "foreignKey": "ownerId"
    }
  },
  "acls": [{
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "login"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "resetPassword"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "logout"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "confirm"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "findOrCreate"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "loginFacebook"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "loginInstegram"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "loginGoogle"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "checkUsername"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {
    "loginFacebook": {
      "accepts": [{
        "arg": "data",
        "type": "object",
        "required": true,
        "description": "",
        "http": {
          "source": "body"
        }
      }],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "",
      "http": [{
        "path": "/facebookLogin",
        "verb": "post"
      }]
    },
    "loginInstegram": {
      "accepts": [{
        "arg": "data",
        "type": "object",
        "required": true,
        "description": "",
        "http": {
          "source": "body"
        }
      }],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "login by instegram account",
      "http": [{
        "path": "/instegramLogin",
        "verb": "post"
      }]
    },
    "loginGoogle": {
      "accepts": [{
        "arg": "data",
        "type": "object",
        "required": true,
        "description": "",
        "http": {
          "source": "body"
        }
      }],
      "returns": [{
        "arg": "result",
        "type": "object",
        "root": true,
        "description": ""
      }],
      "description": "login by google account",
      "http": [{
        "path": "/googleLogin",
        "verb": "post"
      }]
